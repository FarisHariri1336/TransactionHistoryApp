{
  "name": "TransactionHistoryApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --config ./.eslintrc.js --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "jest",
    "format_all": "prettier --write \"{src,tests}/**/*.{tsx,ts,json,js,jsx}\"",
    "clean:android": "cd android/app && rm -rf build",
    "clean:pods": "cd ios && rm -rf pods && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "clean:ios": "npm run clean:pods && cd ios && rm -rf build",
    "clean:modules": "rm -rf node_modules && yarn",
    "install:pods": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "remove:ios:build": "cd ios && rm -rf build",
    "ios:device": "react-native run-ios --device"
  },
  "dependencies": {
    "@react-native-clipboard/clipboard": "^1.16.2",
    "@react-native-community/netinfo": "^11.0.0",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.3.1",
    "lodash.debounce": "4.0.8",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-biometrics": "^3.0.1",
    "react-native-bootsplash": "^6.3.7",
    "react-native-fast-image": "^0.1.1",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-pager-view": "^6.7.1",
    "react-native-progress": "5.0.1",
    "react-native-reanimated": "^3.17.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-toast-message": "^2.3.0",
    "react-native-user-inactivity": "^1.2.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-svg": "^15.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
